apiVersion: v1
clusters:
- cluster:
    certificate-authority: {{ k8s_work_dir }}/cert/ca.pem
    server: {{ KUBE_APISERVER }}
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kubelet-bootstrap
  name: default
current-context: default
kind: Config
preferences: {}
users:
- name: kubelet-bootstrap
  user:
    token: {{ BOOTSTRAP_TOKEN }}
